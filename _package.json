{
  "name": "<%= domainName %>",
  "description": "Website code repo for <%= siteName %>",
  "repository": {
    "type": "git",
    "url": "https://git@bitbucket.org/line58/<%= domainName %>.git"
  },
  "version": "0.1.0",
  "browserify": {
    "transform": [
      "browserify-shim",
      "babelify"
    ]
  },
  "scripts": {
    "dbPullProduction": "./dbPullProduction.sh",
    "dbPullStaging": "./dbPullStaging.sh",
    "dbPushStaging": "./dbPushStaging.sh",
    "watch": "gulp watch",
    "build": "gulp",
    "production": "gulp --production"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "lodash": "./node_modules/lodash",
    "chosen": "./src/js/vendor/chosen.customized.jquery.js",
    "fitVids": "./src/js/vendor/jquery.fitvids.js",
    "wolfy87-eventemitter": "./node_modules/wolfy87-eventemitter/EventEmitter.js",
    "eventie": "./node_modules/eventie/eventie.js",
    "imagesLoaded": "./node_modules/imagesloaded/imagesloaded.js",
    "matchHeight": "./node_modules/jquery-match-height/jquery.matchHeight.js",
    "owlCarousel": "./node_modules/owl.carousel/dist/owl.carousel.js",
    "placeholder": "./node_modules/jquery-placeholder/jquery.placeholder.js",
    "validate": "./src/js/vendor/jquery.validate.js",
    "validateAdditional": "./src/js/vendor/jquery.validate.additional.js",
    "velocity": "./node_modules/velocity-animate/velocity.js",
    "velocity-ui": "./node_modules/velocity-animate/velocity.ui.js"
  },
  "browserify-shim": {
    "owlCarousel": {
      "depends": [
        "jquery:window.jQuery"
      ]
    },
    "fitVids": {
      "depends": [
        "jquery:window.jQuery"
      ]
    },
    "validate": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "validateAdditional": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-runtime": "^6.9.2",
    "babelify": "^7.3.0",
    "browser-sync": "^2.13.0",
    "browserify": "13.0.1",
    "browserify-shim": "^3.8.12",
    "cssnano": "^3.7.3",
    "del": "^2.2.1",
    "fs": "0.0.2",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.0",
    "gulp-eslint": "^3.0.1",
    "gulp-filter": "^4.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.2",
    "gulp-modify": "^0.1.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^7.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-sequence": "^0.4.5",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.4",
    "lazypipe": "^1.0.1",
    "node-bourbon": "^4.2.8",
    "node-neat": "^1.7.2",
    "path": "^0.12.7",
    "postcss-assets": "^4.1.0",
    "postcss-reporter": "^1.4.1",
    "postcss-scss": "^0.1.9",
    "require-dir": "^0.3.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "yargs": "^4.8.0"
  },
  "dependencies": {
    "imagesloaded": "^4.1.0",
    "jquery": "^3.1.0",
    "jquery-match-height": "^0.7.0",
    "jquery-placeholder": "^2.3.1",
    "lodash": "^4.13.1",
    "typi": "^2.2.0",
    "velocity-animate": "^1.2.3",
    "owl.carousel": "git+https://github.com/smashingboxes/OwlCarousel2.git"
  }
}
